#include <stdio.h>
#include <string.h>
int main(){
    char name[20];
    printf("enter your full name: ");
    fgets(name,20,stdin);
    int n=strlen(name);
    int vowel=0,cons=0;
    for(int i=0;i<n;++i){
        if(name[i]=='a'||name[i]=='e'||name[i]=='i'||name[i]=='o'||name[i]=='u'){
            ++vowel;
        }
        else
        ++cons;
    }
    printf("\nvowels: %d",vowel);
    printf("\nconsonants: %d",cons);
    return 0;
}
